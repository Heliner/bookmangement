/* pages/adCheckBook/adCheckBook.wxss */
.logo{
  height:200rpx; 
}

.content{


}
.form{
  margin-top: 20rpx;
  width: 100%;
  height: 100rpx;
}

.search{
  float:left;
  width:70%;
  height: 80rpx;
  margin-top: 10rpx;
  margin-left:2%;
  border: 5rpx #B2DFEE solid;
}
.Btn{
  float: left;
  width:20%;
  margin-left:2%;
  margin-top: 10rpx;
  height: 74rpx;
 
}

.table{
  margin-top: 50rpx;
  width: 98%;
  margin-left: 1%;
  display: flex;
  flex-direction: column;
  border-top: 1rpx solid #419EDC;
  border-left: 1rpx solid #419EDC;
}

.tr{
  
  display: flex;
  flex-direction: row;
}
.thead .td{
  text-align: center;
  font-weight: bold;
  align-content: center;
}

.td{
 text-align: center;
  border-right: 1rpx solid #419EDC;
  border-bottom: 1rpx solid #419EDC;
}
.tr .td:first-child{
  flex:1;
}
.tr .td:nth-child(2){
  flex:3;
}
.tr .td:nth-child(3){
  flex: 2;
}
.tr .td:nth-child(4){
  flex: 2;
}
.tr .td:nth-child(5){
  flex: 2;
}

